@model HalloDocDAL.Model.ViewEncounterForm;@{    Layout = "_adminDash";}@{    ViewData["Title"] = "Encounter Form";}<section>    <div class="w-100 p-3 d-flex flex-column align-items-center">        <div class="req_form_container w-50 d-flex justify-content-between mt-4 mb-3">            <div class="d-flex">                <div class="d-flex align-items-center me-3">                    <h4 class="fw-semibold  ">Encounter Form</h4>                </div>            </div>            <div>                <a class="back_btn d-flex align-items-center p-2 px-4 fw-semibold" asp-controller="AdminDashboard" asp-action="AdminDashboard" role="button">                    <p class="m-0"><i class="fa-solid fa-chevron-left"></i>&nbsp; Back</p>                </a>            </div>        </div>        <div class="req_form_container w-50 shadow-lg p-3">            <h2 class="text-center fw-bolder my-3 text-info">Medical Report - Confidential</h2>            <form id="encounterform" method="post">                <input asp-for=RequestId class="d-none" />                <div class="row gx-2">                    <div class="col-md-6">                        <div class="form-floating mb-3">                            <input asp-for=FirstName type="text" class="form-control" id="first_name" placeholder="First Name">                            <label for="floatingInput" class="floating-label">First Name</label>                        </div>                    </div>                    <div class="col-md-6">                        <div class="form-floating mb-3">                            <input asp-for=LastName type="text" class="form-control" id="last_name" placeholder="Last Name">                            <label for="last_name" class="floating-label">Last Name</label>                        </div>                    </div>                </div>                <div class="row gx-2">                    <div class="col-md-12">                        <div class="form-floating mb-3">                            <input asp-for=Location type="text" class="form-control" id="location" placeholder="Location">                            <label for="location" class="floating-label">Location</label>                        </div>                    </div>                </div>                <div class="row gx-2">                    <div class="col-md-6">                        <div class="form-floating mb-3">                            <input asp-for=DateOfBirth type="date" class="form-control" id="dobe" placeholder="Date of Birth">                            <label for="dob" class="floating-label">Date of Birth</label>                        </div>                    </div>                    <div class="col-md-6">                        <div class="form-floating mb-3">                            <input asp-for=DateOfService type="date" class="form-control" id="last_name" placeholder="Date">                            <label for="Date" class="floating-label">Date</label>                        </div>                    </div>                </div>                <div class="row gx-2">                    <div class="col-md-6">                        <div class="form-floating mb-3">                            <input asp-for=Email type="text" class="form-control" id="email" placeholder="Email">                            <label for="email" class="floating-label">Email</label>                        </div>                    </div>                    <div class="col-md-6">                        <div class="form-floating mb-3">                            <input asp-for=PhoneNumber type="tel" class="form-control" id="phone" placeholder="Phone Number">                            <label for="phone" class="floating-label">Phone Number</label>                        </div>                    </div>                </div>                <div class="row gx-2">                    <div class="col-md-6">                        <div class="form-floating mb-3">                            <input asp-for=HistoryOfPresentIllness type="text" class="form-control" id="historyillness" placeholder="History Of Present Ilness Or Injury">                            <label for="historyillness" class="floating-label">History Of Present Ilness Or Injury</label>                        </div>                    </div>                    <div class="col-md-6">                        <div class="form-floating mb-3">                            <input asp-for=MedicalHistory type="text" class="form-control" id="medicalhistory" placeholder="Medical History">                            <label for="medicalhistory" class="floating-label">Medical History</label>                        </div>                    </div>                </div>                <div class="row gx-2">                    <div class="col-md-6">                        <div class="form-floating mb-3">                            <input asp-for=Medications type="text" class="form-control" id="medications" placeholder="Medications">                            <label for="medications" class="floating-label">Medications</label>                        </div>                    </div>                    <div class="col-md-6">                        <div class="form-floating mb-3">                            <input asp-for=Allergies type="text" class="form-control" id="allergies" placeholder="Allergies">                            <label for="allergies" class="floating-label">Allergies</label>                        </div>                    </div>                </div>                <div class="row gx-2">                    <div class="col-md-4">                        <div class="form-floating mb-3">                            <input asp-for=Temperature type="text" class="form-control" id="temperature" placeholder="Temperature">                            <label for="temperature" class="floating-label">Temperature</label>                        </div>                    </div>                    <div class="col-md-4">                        <div class="form-floating mb-3">                            <input asp-for=HR type="text" class="form-control" id="hr" placeholder="HR">                            <label for="hr" class="floating-label">HR</label>                        </div>                    </div>                    <div class="col-md-4">                        <div class="form-floating mb-3">                            <input asp-for=RR type="text" class="form-control" id="rr" placeholder="RR">                            <label for="rr" class="floating-label">RR</label>                        </div>                    </div>                </div>                <div class="row gx-2">                    <div class="col-md-2">                        <div class="form-floating mb-3">                            <input asp-for=BPSystolic type="text" class="form-control" id="bpsystolic" placeholder="Blood Presssure">                            <label for="bpsystolic" class="floating-label">BP (Systolic)</label>                        </div>                    </div>                    <div class="col-md-2">                        <div class="form-floating mb-3">                            <input asp-for=BPDiastolic type="text" class="form-control" id="bpdiastolic" placeholder="Blood Presssure">                            <label for="bpdiastolic" class="floating-label">BP (Diastolic)</label>                        </div>                    </div>                    <div class="col-md-4">                        <div class="form-floating mb-3">                            <input asp-for=O2 type="text" class="form-control" id="oxygen" placeholder="O2">                            <label for="oxygen" class="floating-label">O2</label>                        </div>                    </div>                    <div class="col-md-4">                        <div class="form-floating mb-3">                            <input asp-for=Pain type="text" class="form-control" id="pain" placeholder="Pain">                            <label for="pain" class="floating-label">Pain</label>                        </div>                    </div>                </div>                <div class="row gx-2">                    <div class="col-md-6">                        <div class="form-floating mb-3">                            <input asp-for=Heent type="text" class="form-control" id="heent" placeholder="HEENT">                            <label for="heent" class="floating-label">HEENT</label>                        </div>                    </div>                    <div class="col-md-6">                        <div class="form-floating mb-3">                            <input asp-for=CV type="text" class="form-control" id="cv" placeholder="CV">                            <label for="cv" class="floating-label">CV</label>                        </div>                    </div>                </div>                <div class="row gx-2">                    <div class="col-md-6">                        <div class="form-floating mb-3">                            <input asp-for=Chest type="text" class="form-control" id="chest" placeholder="Chest">                            <label for="chest" class="floating-label">Chest</label>                        </div>                    </div>                    <div class="col-md-6">                        <div class="form-floating mb-3">                            <input asp-for=ABD type="text" class="form-control" id="abd" placeholder="ABD">                            <label for="abd" class="floating-label">ABD</label>                        </div>                    </div>                </div>                <div class="row gx-2">                    <div class="col-md-6">                        <div class="form-floating mb-3">                            <input asp-for=Extr type="text" class="form-control" id="extr" placeholder="Extr">                            <label for="extr" class="floating-label">Extr</label>                        </div>                    </div>                    <div class="col-md-6">                        <div class="form-floating mb-3">                            <input asp-for=Skin type="text" class="form-control" id="skin" placeholder="Skin">                            <label for="skin" class="floating-label">Skin</label>                        </div>                    </div>                </div>                <div class="row gx-2">                    <div class="col-md-6">                        <div class="form-floating mb-3">                            <input asp-for=Neuro type="text" class="form-control" id="neuro" placeholder="Neuro">                            <label for="neuro" class="floating-label">Neuro</label>                        </div>                    </div>                    <div class="col-md-6">                        <div class="form-floating mb-3">                            <input asp-for=Other type="text" class="form-control" id="other" placeholder="Other">                            <label for="other" class="floating-label">Other</label>                        </div>                    </div>                </div>                <div class="row gx-2">                    <div class="col-md-6">                        <div class="form-floating mb-3">                            <input asp-for=Diagnosis type="text" class="form-control" id="diagnosis" placeholder="Diagnosis">                            <label for="diagnosis" class="floating-label">Diagnosis</label>                        </div>                    </div>                    <div class="col-md-6">                        <div class="form-floating mb-3">                            <input asp-for=TreatmentPlan type="text" class="form-control" id="treatmentplan" placeholder="Treatment Plan">                            <label for="treatmentplan" class="floating-label">Treatment Plan</label>                        </div>                    </div>                </div>                <div class="row gx-2">                    <div class="col-md-6">                        <div class="form-floating mb-3">                            <input type="text" class="form-control" id="medicationsdispensed" placeholder="Medications Dispensed">                            <label for="medicationsdispensed" class="floating-label">Medications Dispensed</label>                        </div>                    </div>                    <div class="col-md-6">                        <div class="form-floating mb-3">                            <input type="text" class="form-control" id="procedures" placeholder="Procedures">                            <label for="procedures" class="floating-label">Procedures</label>                        </div>                    </div>                </div>                <div class="row gx-2">                    <div class="col-md-12">                        <div class="form-floating mb-3">                            <input type="text" class="form-control" id="followup" placeholder="Followup">                            <label for="followup" class="floating-label">Followup</label>                        </div>                    </div>                    <input asp-for=IsFinalized id="IsFinalized" type="number" class="d-none" />                </div>                <div class="d-flex flex-row-reverse">                    <a class="back_btn d-flex align-items-center p-2 px-3 fw-semibold" asp-controller="AdminDashboard" asp-action="Index" role="button">                        <p class="m-0">Cancel</p>                    </a>                    <button type="button" data-bs-toggle="modal" data-bs-target="#finalizemodal" class="btn p-2 mx-1" style="background-color:indigo; color:white">Finalize</button>                    <button id="normalsubmit" type="button" class="btn btn-info text-light mx-1">Save Changes</button>                </div>            </form>        </div>    </div>    <div class="modal fade" id="finalizemodal" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true">        <div class="modal-dialog modal-dialog-centered">            <div class="modal-content">                <div class="modal-header bg-info">                    <h1 class="modal-title text-light  fs-5" id="exampleModalLabel">Confirm Finalization</h1>                    <button type="button" class="btn-close btn-light" data-bs-dismiss="modal" aria-label="Close"></button>                </div>                <div class="modal-body">                    <form method="post">                        <p>Are you sure want to Finalize??</p>                        <div class="modal-footer">                            <button id="finalizesubmit" type="submit" class="btn btn-info text-light">Confirm</button>                            <button type="button" class="btn btn-outline-info" data-bs-dismiss="modal">Cancel</button>                        </div>                    </form>                </div>            </div>        </div>    </div></section><script>    const finalizemodal = document.getElementById('finalizemodal')    if (finalizemodal) {        finalizemodal.addEventListener('show.bs.modal', event => {            const button = event.relatedTarget        })    }    $("#encounterform").on("submit", function (e) {        e.preventDefault();        var formData = $(this).serialize();        $.ajax({            type: 'POST',            url: "/AdminDashboard/EditEncounterForm",            data: formData,            success: function (response) {                if (response.code == 401) {                    location.reload();                }                else {                    console.log(response);                    if (response == true) {                    }                }            },            error: function () {                console.log("Function Fail")            }        });    })    $("#normalsubmit").on("click", function (e) {        e.preventDefault();        $("#IsFinalized").val(0);        $("#encounterform").submit()        location.reload();    })    $("#finalizesubmit").on("click", function (e) {        e.preventDefault();        $("#IsFinalized").val(1);        $("#encounterform").submit();        window.location.href = "/AdminDashboard/AdminDashboard";        toastr.success("Data updated Successfully.");    });</script>