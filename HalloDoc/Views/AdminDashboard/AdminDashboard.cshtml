            <div class="modal-body">
                <form id="formSendLink" method="post">
                    <div class="form-floating required mb-3">
                        <input type="text" class=" form-control" id="fname" name="fname" placeholder=" ">
                        <label for="fname" class=" floating-label">First Name</label>
                        <span style="color:red; display:none">Required!</span>
                    </div>
                    <div class="form-floating required mb-3">
                        <input type="text" class=" form-control" id="lname" name="lname" placeholder=" ">
                        <label for="fname" class=" floating-label">Last Name</label>
                        <span style="color:red; display:none">Required!</span>
                    </div>
                    <div class="form-floating mb-3 h-100 z-3 required">
                        <input type="tel" class=" form-control h-100" id="tel" name="tel" placeholder="Phone Number">
                        <span style="color:red; display:none">Required!</span>
                    </div>
                    <div class="form-floating mb-3 required">
                        <input type="email" class=" form-control" id="email" name="email" placeholder=" ">
                        <label for="email" class=" floating-label">Email address</label>
                        <span style="color:red; display:none">Required!</span>
                    </div>
                    <div class="text-end">
                        <button type="submit" class="btn btn-info text-white">Confirm</button>
                        <button type="button" class="btn border border-info text-info" data-bs-dismiss="modal">Cancel</button>
                    </div>
                </form>
            </div>
        </div>
    </div>
</div>
<script>
    document.addEventListener('DOMContentLoaded', function () {
        const buttons = document.querySelectorAll('.nav-link');

        buttons.forEach(button => {
            button.addEventListener('click', function () {

                const arrowDown = this.nextElementSibling.querySelector('.arrow-down');
                document.querySelectorAll('.arrow-down').forEach(arrow => arrow.style.display = 'none');
                arrowDown.style.display = this.classList.contains('active') ? 'block' : 'none';
            });
        });
    });

    document.addEventListener('DOMContentLoaded', function () {
        const newTabButton = document.getElementById('new-tab');
        newTabButton.click();
    });

    function filterrequests(reqtype) {
        table = document.getElementsByClassName("caseTable");
        for (j = 0; j < table.length; j++) {
            tr = table[j].getElementsByTagName("tr");
            for (i = 0; i < tr.length; i++) {
                td = tr[i].getElementsByTagName("td")[0];
                if (td) {
                    intValue = td.innerText;
                    //indexOf will return the position of first matched character
                    if (intValue == reqtype) {
                        tr[i].style.display = "";
                    }
                    else {
                        tr[i].style.display = "none";
                    }
                }
            }
        }
    }

    function allrequests() {
        table = document.getElementsByClassName("caseTable");
        for (i = 0; i < table.length; i++) {
            tr = table[i].getElementsByTagName("tr");
            for (j = 0; j < tr.length; j++) {
                tr[j].style.display = "";
            }
        }
        
    }

    function search() {
        var input, filter, table, tr, td, i, txtValue;
        input = document.getElementById("search");
        filter = input.value.toUpperCase();
        table = document.getElementById("adminTable");
        tr = table.getElementsByTagName("tr");
        for (i = 0; i < tr.length; i++) {
            td = tr[i].getElementsByTagName("td")[2];
            if (td) {
                txtValue = td.textContent || td.innerText;
                if (txtValue.toUpperCase().indexOf(filter) > -1) {
                    tr[i].classList.remove("d-none");
                } else {
                    tr[i].classList.add("d-none");;
                }
            }
        }
    }

    function regionFilter() {
        var region = document.getElementById("region");
        table = document.getElementById("adminTable");
        tr = table.getElementsByTagName("tr");
        if (region.value == 0) {
            for (i = 0; i < tr.length; i++) {
                td = tr[i].getElementsByTagName("td")[1];
                if (td) {
                    tr[i].classList.remove("region-filter");
                }
            }
        }
        else {
            for (i = 0; i < tr.length; i++) {
                td = tr[i].getElementsByTagName("td")[1];
                if (td) {
                    txtValue = td.textContent || td.innerText;
                    if (txtValue.toUpperCase() == region.value) {
                        tr[i].classList.remove("region-filter");
                    } else {
                        tr[i].classList.add("region-filter");;
                    }
                }
            }
        }
        
    }

